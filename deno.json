{
  "name": "@agentcards/agentcards",
  "version": "0.1.0",
  "exports": "./mod.ts",
  "tasks": {
    "dev": "deno run --watch --allow-all src/main.ts",
    "test": "deno test --allow-all",
    "bench": "deno bench --allow-all",
    "lint": "deno lint",
    "fmt": "deno fmt",
    "check": "deno check src/**/*.ts",
    "build": "deno compile --allow-all --output=agentcards src/main.ts"
  },
  "fmt": {
    "useTabs": false,
    "lineWidth": 100,
    "semiColons": true,
    "indentWidth": 2
  },
  "lint": {
    "rules": {
      "tags": ["recommended"]
    }
  },
  "imports": {
    "@std/assert": "jsr:@std/assert@1.0.11",
    "@std/log": "jsr:@std/log@0.224.11",
    "@std/yaml": "jsr:@std/yaml@1.0.6"
  },
  "compilerOptions": {
    "strict": true,
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true
  }
}
